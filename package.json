{
  "name": "zprofile",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 8080",
    "start:local": "next start -p 3000",
    "prettier": "prettier *.json *.js \"src/**/*.{js,jsx,ts,tsx}\" \".circleci/config.yml\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "ava",
    "test:integration": "npm run cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "hasura": "hasura --project ./hasura",
    "seeds:generate": "ts-node ./hasura/seeds/seedscript.ts",
    "seeds:apply": "npm run hasura seeds apply"
  },
  "dependencies": {
    "@apollo/client": "3.4.7",
    "@auth0/auth0-react": "1.6.0",
    "await-of": "3.1.1",
    "graphql": "15.5.1",
    "next": "11.1.0",
    "next-pwa": "5.2.24",
    "object-path": "0.11.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-responsive": "8.2.0",
    "react-swipeable": "6.1.2",
    "use-debounce": "7.0.0"
  },
  "devDependencies": {
    "@types/faker": "5.5.7",
    "@types/node": "16.6.1",
    "@types/object-path": "0.11.1",
    "@types/react": "17.0.17",
    "autoprefixer": "^10.2.6",
    "ava": "3.15.0",
    "cypress": "8.2.0",
    "faker": "5.5.3",
    "hasura-cli": "2.0.5",
    "json-sql": "0.5.0",
    "postcss": "8.3.6",
    "prettier": "2.3.2",
    "tailwindcss": "2.2.7",
    "tailwindcss-css-filters": "^2.1.0",
    "ts-node": "10.2.0",
    "typescript": "4.3.5"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
