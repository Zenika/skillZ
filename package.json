{
  "name": "zprofile",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:local": "npm run generate:local && npm run build",
    "build:prod": "npm run generate && npm run build",
    "build:e2e": "npm run generate && NODE_ENV=test next build",
    "start": "next start -p 8080",
    "start:local": "next start -p 3000",
    "prettier": "prettier *.json *.js \"src/**/*.{js,jsx,ts,tsx}\" \".circleci/config.yml\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "ava",
    "test:integration": "npm run cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "hasura": "hasura --project ./hasura",
    "seeds:generate": "ts-node ./hasura/local_seeds/seedscript.ts",
    "seeds:apply": "npm run hasura seeds apply --file ./hasura/local_seeds/seeds.sql",
    "generate": "graphql-codegen --config codegen.js",
    "generate:local": "graphql-codegen -r dotenv/config --config codegen.js dotenv_config_path=.env.local "
  },
  "dependencies": {
    "@apollo/client": "^3.5.7",
    "@auth0/auth0-react": "1.6.0",
    "await-of": "3.1.1",
    "graphql": "15.8.0",
    "next": "^12.0.7",
    "next-pwa": "^5.4.4",
    "object-path": "^0.11.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-responsive": "8.2.0",
    "react-swipeable": "6.1.2",
    "sharp": "^0.30.7",
    "use-debounce": "7.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.8.0",
    "@graphql-codegen/introspection": "^2.2.0",
    "@graphql-codegen/typescript": "^2.7.1",
    "@graphql-codegen/typescript-operations": "^2.5.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.1",
    "@types/faker": "5.5.7",
    "@types/node": "16.6.1",
    "@types/object-path": "0.11.1",
    "@types/react": "17.0.17",
    "autoprefixer": "^10.4.2",
    "ava": "^4.0.1",
    "cypress": "^10.7.0",
    "dotenv": "^10.0.0",
    "faker": "5.5.3",
    "hasura-cli": "^2.8.4",
    "json-sql": "^0.5.0",
    "postcss": "^8.4.5",
    "prettier": "2.3.2",
    "tailwindcss": "^3.0.13",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "overrides": {
    "underscore": ">=1.12.1"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
