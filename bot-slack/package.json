{
    "name": "skillz-bot",
    "version": "1.1.1",
    "description": "basic bot for Skillz app",
    "main": "app.js",
    "scripts": {
        "dev": "node -r dotenv/config app.js",
        "start": "node app.js",
        "test": "jest",
        "coverage": "coverage app.js",
        "format": "prettier *.json *.js \"src/**/*.{js,jsx,ts,tsx}\"",
        "format:check": "npm run format -- --check",
        "format:write": "npm run format -- --write"
    },
    "dependencies": {
        "@slack/bolt": "^3.12.2",
        "body-parser": "^1.19.1",
        "coverage": "0.4.1",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "lib": "^4.3.3",
        "node-cron": "^3.0.0",
        "node-fetch": "2.6",
        "request": "^2.88.2",
        "request-promise": "^4.2.6"
    },
    "engines": {
        "node": "22.x"
    },
    "repository": {
        "url": "https://glitch.com/edit/#!/bolt-app-template"
    },
    "license": "MIT",
    "keywords": [
        "node",
        "express"
    ],
    "devDependencies": {
        "jest": "^30.0.3",
        "prettier": "2.8.4"
    }
